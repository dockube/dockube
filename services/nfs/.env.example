# ================================================================================================
# options: 14.04 / 16.04 (default)
UBUNTU_VERSION=16.04
MASTER_PORT=221

# ================================================================================================
#   CONTAINER: NODE (UBUNTU)
# ================================================================================================
NODE_PORT=223

# ================================================================================================
#   CONTAINER: WORKSPACE
# ================================================================================================
WORKSPACE_PORT=227
WORKSPACE_GOLANG_PORT=228

WORKSPACE_INSTALL_GOLANG=true